{
  "name": "@phothinmaung/julian-period",
  "version": "0.1.1",
  "description": "The utilities tools for working with JD , JDN and Astronomical Cycles: [Solar, Metonic (Lunar),Indiction]",
  "scripts": {
    "build": "tsx src/opt/build.ts",
    "check": "biome check src",
    "lint": "biome check --write src",
    "fmt": "biome format --write src"
  },
  "keywords": [
    "julian-day",
    "julian-day-number",
    "JD",
    "JDN"
  ],
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "browser": "cdn/julianPeriod.js",
  "author": {
    "name": "Pho Thin Mg",
    "email": "phothinmg@disroot.org",
    "url": "https://phothinmg.github.io/"
  },
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^24.10.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/*",
    "package.json",
    "README.md",
    "cdn/*",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phothinmg/julian-period.git"
  }
}
